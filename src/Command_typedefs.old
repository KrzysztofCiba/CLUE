#ifndef COMMAND_TYPEDEFS_H
#define COMMAND_TYPEDEFS_H

#include <complex>

typedef IntCmd    CLUE::Command<int>;
typedef FloatCmd  CLUE::Command<float>;
typedef DoubleCmd CLUE::Command<double>;
typedef CmplxFCmd CLUE::Command< complex < float > >;
typedef CmplxDCmd CLUE::Command< complex < double > >;
typedef CharCmd   CLUE::Command<char*>;

#endif // COMMAND_TYPEDEFS_H
